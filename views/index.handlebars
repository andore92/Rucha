<div class="row">
  <div class="col-sm-2" id="chat-sidebar">
    <div class="sidebar-nav-fixed affix">
      {{userinChatroom}}
      <h1 id="room-header">{{this.croom}}</h1>
        <input type="hidden" id="loggedUser" value= {{this.loginUser}}>
        <input type="hidden" id="chatroom" value= {{this.croom}}>  
    </div>                      
  </div>{{!-- close sidebar --}}
<div class="col-sm-10">
    <div id="message-div">
      <br>
      <ul id="messages"></ul>
      <br>
    </div>
    <br>
    <br>
    <br>
    <span id="notifyUser"></span>
    <div class="navbar-fixed-bottom" id="form">
      <form id="form" action="" onsubmit="return submittoChatroomfunction();"> 
           <input type="hidden" id="looser_user" value= {{userinChatroom.loginUser}} /><input id="msgbox" autocomplete="off" onkeyup="notifyTyping();" placeholder="Type yor message here.." /><input type="submit" id="button" value="Send"/> 
      </form>
    </div>{{!-- close navbar-fixed-bottom --}}
  </div>{{!-- close col-sm-10 --}}
</div>{{!-- close row --}}

